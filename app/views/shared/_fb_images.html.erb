<% if user_signed_in? %>
    <ul class="thumbnails">
      <li class="span6 ga-image-gallery">
        <% unless $photos.blank? %>
            <% $photos.each do |photo| %>
                <%= image_tag "#{photo}", :class => "img-rounded draggable", :onclick => "choose_fb_image('#{photo}')" ,:width => 50 %>
            <% end %>
        <% end %>
      </li>
    </ul>
<% end %>